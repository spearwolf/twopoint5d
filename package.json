{
  "name": "@spearwolf/twopoint5d",
  "description": "a library to create 2.5d realtime graphics and pixelart with three.js",
  "version": "0.0.0",
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=20.12",
    "pnpm": ">=9.7"
  },
  "packageManager": "pnpm@9.7",
  "scripts": {
    "build": "pnpm nx run-many -t build",
    "test": "pnpm nx run-many -t test",
    "lint": "pnpm nx run-many -t lint",
    "test:affected": "pnpm nx affected -t test",
    "clean": "pnpm nx run-many -t clean && rimraf dist",
    "publishNpmPkg": "pnpm nx run-many -t publishNpmPkg",
    "update": "pnpm dlx npm-check --update",
    "cbt": "pnpm run-s -sn clean build test",
    "lookbook": "pnpm nx dev lookbook",
    "landing-page": "pnpm nx dev landing-page",
    "ci": "pnpm run-s -sn build lint test",
    "playwright:install": "pnpm exec playwright install",
    "ci:install": "pnpm run-s -sn playwright:install ci"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@nx/eslint-plugin": "^19.3.0",
    "@nx/jest": "^19.3.0",
    "@nx/js": "^19.3.0",
    "@nx/linter": "^19.3.0",
    "@react-three/fiber": "^8.16.8",
    "@react-three/test-renderer": "^8.2.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@spearwolf/eventize": "^3.4.2",
    "@spearwolf/signalize": "^0.13.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/sinon": "^17.0.3",
    "@types/three": "~0.165.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "canvas": "^2.11.2",
    "esbuild": "^0.21.5",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-require-extensions": "^0.1.3",
    "happy-dom": "^14.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "nx": "^19.3.0",
    "playwright": "^1.44.1",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "react": "^18.3.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "sinon": "^18.0.0",
    "three": "~0.165.0",
    "ts-jest": "^29.1.4",
    "ts-node": "10.9.2",
    "tslib": "^2.6.3",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "use-asset": "^1.0.4",
    "vitest": "^1.6.0",
    "zustand": "^4.5.2"
  }
}
