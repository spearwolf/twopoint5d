{
  "name": "twopoint5d-r3f",
  "version": "0.1.0-dev",
  "description": "A refreshingly satisfying 2.5D graphics library built on @react-three/fiber",
  "main": "twopoint5d-r3f.cjs.js",
  "module": "twopoint5d-r3f.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./twopoint5d-r3f.js",
    "require": "./twopoint5d-r3f.cjs.js"
  },
  "sideEffects": false,
  "rollupBuild": {
    "outputDir": ".",
    "outputName": "twopoint5d-r3f",
    "externals": [
      "@react-three/fiber",
      "@spearwolf/eventize",
      "react-dom",
      "react",
      "react/jsx-runtime",
      "three",
      "twopoint5d",
      "use-asset",
      "zustand"
    ],
    "esm": {
      "buildName": "es2017"
    },
    "cjs": {
      "buildName": "cjs"
    }
  },
  "homepage": "https://github.com/spearwolf/three-vertex-objects/",
  "repository": {
    "type": "git",
    "url": "https://github.com/spearwolf/three-vertex-objects.git"
  },
  "bugs": {
    "url": "https://github.com/spearwolf/three-vertex-objects/issues"
  },
  "author": {
    "name": "Wolfger Schramm",
    "email": "wolfger@spearwolf.de",
    "url": "https://www.spearwolf.de"
  },
  "scripts": {
    "build:js": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider rollup -c ../../shared-rollup-build/rollup.config.esm.js",
    "watch:js": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider rollup -w -c ../../shared-rollup-build/rollup.config.esm.js",
    "build:cjs": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider rollup -c ../../shared-rollup-build/rollup.config.cjs.js",
    "watch:cjs": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider rollup -w -c ../../shared-rollup-build/rollup.config.cjs.js",
    "build:types": "tsc --emitDeclarationOnly",
    "watch:types": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "watch": "concurrently --names types,js yarn:watch:types yarn:watch:js",
    "build": "concurrently --group --names js,cjs,types yarn:build:js yarn:build:cjs yarn:build:types",
    "clean": "rimraf build types dist .rollup.cache tsconfig.tsbuildinfo twopoint5d-r3f.js twopoint5d-r3f.js.map twopoint5d-r3f.cjs.js twopoint5d-r3f.cjs.js.map",
    "eslint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "fix": "run-s --silent --print-label prettier:write eslint:fix",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "cbt": "run-s --silent --print-label clean build test"
  },
  "peerDependencies": {
    "@react-three/fiber": "^8.10.0",
    "@spearwolf/eventize": "^2.0.3",
    "react": "^18.2.0",
    "three": "^0.149",
    "twopoint5d": "*",
    "use-asset": "^1.0.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@react-three/fiber": "^8.10.4",
    "@react-three/test-renderer": "^8.1.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@spearwolf/eventize": "^2.0.4",
    "@testing-library/react": "^13.4.0",
    "@types/assert": "^1.5.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/react": "^18.0.27",
    "@types/react-reconciler": "^0.28.2",
    "@types/sinon": "^10.0.13",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "concurrently": "^7.6.0",
    "core-js": "^3.27.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-problems": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "scheduler": "^0.23.0",
    "sinon": "^15.0.1",
    "three": "^0.149.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "twopoint5d": "workspace:*",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.5",
    "use-asset": "^1.0.4",
    "zustand": "^4.3.2"
  }
}
