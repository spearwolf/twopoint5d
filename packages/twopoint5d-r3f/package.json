{
  "name": "@spearwolf/twopoint5d-r3f",
  "description": "a 2.5D graphics library built on @react-three/fiber and three.js",
  "version": "0.1.0-dev",
  "author": {
    "name": "Wolfger Schramm",
    "email": "wolfger@spearwolf.de",
    "url": "https://www.spearwolf.de"
  },
  "license": "Apache-2.0",
  "main": "dist/twopoint5d-r3f.js",
  "type": "module",
  "module": "dist/twopoint5d-r3f.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/twopoint5d-r3f.js",
    "require": "./dist/twopoint5d-r3f.cjs.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "run-s compile bundle makePackageJson",
    "compile": "tsc -b tsconfig.build.json",
    "bundle": "rollup -c rollup.config.js",
    "clean": "rimraf build types dist ../../dist/packages/twopoint5d-r3f",
    "makePackageJson": "node ../../scripts/makePackageJson.mjs"
  },
  "peerDependencies": {
    "@react-three/fiber": "^8.13.0",
    "@spearwolf/eventize": "^3.0.0",
    "@spearwolf/twopoint5d": "*",
    "react": "^18.2.0",
    "three": "^0.153",
    "use-asset": "^1.0.4",
    "zustand": "^4.3.8"
  },
  "rollup": {
    "external": [
      "@react-three/fiber",
      "@spearwolf/eventize",
      "@spearwolf/twopoint5d",
      "react-dom",
      "react",
      "react/jsx-runtime",
      "three",
      "use-asset",
      "zustand"
    ]
  }
}
