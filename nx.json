{
  "parallel": 6,
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "lint": {
      "executor": "nx:run-script",
      "options": {
        "script": "lint"
      },
      "cache": true
    },
    "test": {
      "executor": "nx:run-script",
      "options": {
        "script": "test"
      },
      "cache": true
    },
    "publishNpmPkg": {
      "executor": "nx:run-script",
      "dependsOn": [
        "build"
      ],
      "options": {
        "script": "publishNpmPkg"
      }
    }
  },
  "defaultBase": "main"
}
